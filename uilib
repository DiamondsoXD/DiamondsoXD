local Uilibrary = {}
function Uilibrary:CreateButton(text, callback)
    local callback =  callback or function() end
    local TextButton = Instance.new("TextButton")

    TextButton.Parent = ScrollingFrame
    TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextButton.Position = UDim2.new(0.106164381, 0, 0, 0)
    TextButton.Size = UDim2.new(0, 200, 0, 50)
    TextButton.Font = Enum.Font.SourceSans
    TextButton.Text = text
    TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextButton.TextScaled = true
    TextButton.TextSize = 14.000
    TextButton.TextWrapped = true
    TextButton.TextXAlignment = Enum.TextXAlignment.Right
    TextButton.MouseButton1Down:Connect(function()
        pcall(callback)
    end)
    function Uilibrary:CreateToggle(text, callback)
    local actions = {}
    text = text or "New toggle"
    callback = callback or function() end
    local enabled = false
    local Toggle = Instance.new("TextLabel")
    local Background = Instance.new("TextButton")
    local UICorner = Instance.new("UICorner")
    local Onofftoggled = Instance.new("TextButton")
    local UICorner_2 = Instance.new("UICorner")

    Toggle.Name = "Toggle"
    Toggle.Parent = ScrollingFrame
 
    Toggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Toggle.BackgroundTransparency = 1.000
    Toggle.Size = UDim2.new(0, 200, 0, 50)
    Toggle.Font = Enum.Font.Roboto
    Toggle.Text = text
    Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
    Toggle.TextSize = 30.000
    Toggle.TextXAlignment = Enum.TextXAlignment.Left

    Background.Name = "Background"
    Background.Parent = Toggle
    Background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Background.Position = UDim2.new(0.744000018, 0, 0.200000003, 0)
    Background.Size = UDim2.new(0, 64, 0, 30)
    Background.Font = Enum.Font.SourceSans
    Background.TextColor3 = Color3.fromRGB(0, 0, 0)
    Background.TextSize = 14.000
    Background.TextTransparency = 1.000

    UICorner.Parent = Background

    Onofftoggled.Name = "Onofftoggled"
    Onofftoggled.Parent = Background
    Onofftoggled.BackgroundColor3 = Color3.fromRGB(255, 74, 29)
    Onofftoggled.Position = UDim2.new(-0.004,0,0)
    Onofftoggled.Size = UDim2.new(0, 30, 0, 30)
    Onofftoggled.Font = Enum.Font.SourceSans
    Onofftoggled.TextColor3 = Color3.fromRGB(0, 0, 0)
    Onofftoggled.TextSize = 14.000
    Onofftoggled.TextTransparency = 1.000

    UICorner_2.CornerRadius = UDim.new(0, 6)
    UICorner_2.Parent = Onofftoggled
    local function fire()
        enabled = not enabled
  Onofftoggled:TweenPosition(enabled and UDim2.new(0.528,0,0) or UDim2.new(-0.004,0,0), "InOut", "Linear", 0.1)
  Onofftoggled.BackgroundColor3 = enabled and Color3.fromRGB(46,255,88) or Color3.fromRGB(255,55,37)
        pcall(callback, enabled)
    end
    Onofftoggled.MouseButton1Down:Connect(fire)
    function actions:Set(arg)
    
 Onofftoggled:TweenPosition(enabled and UDim2.new(0.528,0,0) or UDim2.new(-0.004,0,0), "InOut", "Linear", 0.1)
Onofftoggled.BackgroundColor3 = enabled and Color3.fromRGB(46,255,88) or Color3.fromRGB(255,55,37)
        pcall(callback, arg)
    end
end
end
return Uilibrary
end

