Topmain.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
Topmain.Position = UDim2.new(0.348155618, 0, 0.230406061, 0)
Topmain.Size = UDim2.new(0, 600, 0, 50)

container.Name = "container"
container.Parent = Topmain
container.BackgroundColor3 = Color3.fromRGB(102, 102, 102)
container.Position = UDim2.new(-0.000177714042, 0, 1.14706457, 0)
container.Size = UDim2.new(0, 600, 0, 450)

DropShadowHolder.Name = "DropShadowHolder"
DropShadowHolder.Parent = container
